#include "S_velocidade.h"

#define encoder 2

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  attachInterrupt(digitalPinToInterrupt(encoder), medirRPM, RISING);
}

void loop() {
  // put your main code here, to run repeatedly:
  Serial.printf("RPM:");
  medirRPM();
  Serial.println(RPM);

}
